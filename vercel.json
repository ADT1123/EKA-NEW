{
  "buildCommand": "cd website && npm run build",
  "outputDirectory": "website/dist",
  "functions": {
    "api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri"
  }
}
